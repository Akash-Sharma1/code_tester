{
	"name": "code-tester",
	"displayName": "Competitve Companion",
	"description": "This is your Competitive Companion, with this extension you can make test cases for your cpp file, run any files on them and see the result of your submission.",
	"version": "0.0.5",
	"icon": "images/code.png",
	"publisher": "Akashsharma01",
	"repository": { "type": "git", "url": "https://github.com/Akash-Sharma1/code_tester.git" },
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:code-tester.creategenerater",
		"onCommand:code-tester.runtests",
		"onCommand:code-tester.customgentest"
	],
	"main": "extension.js",
	"contributes": {
		"commands": [
			{
				"command": "code-tester.creategenerater",
				"title": "Create Tests"
			},
			{
				"command": "code-tester.runtests",
				"title": "Run Tests"
			},
			{
				"command": "code-tester.customgentest",
				"title": "Create Custom Tests"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"compile-run": "^2.3.2",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
